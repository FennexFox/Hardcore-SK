	<ThingDef ParentName="TreewoodBase">
		<thingClass>SK.PlantMarsh</thingClass>
		<defName>PlantMangrove</defName>
		<label>mangrove</label>
		<description>Mangroves are trees or shrubs that grow in salty water in hot places like the tropics.</description>
		<graphicData>
			<texPath>Things/Plant/MangroveTree</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<shadowData>
				<volume>(0.15, 0.2, 0.15)</volume>
				<offset>(0,0,0)</offset>
			</shadowData>
		</graphicData>
		<statBases>
			<Beauty>2</Beauty>
		</statBases>
		<plant>
			<visualSizeRange>
				<min>1.5</min>
				<max>3.0</max>
			</visualSizeRange>
			<growDays>17</growDays>
			<harvestWork>1500</harvestWork>
			<harvestYield>25</harvestYield>
			<reproduceMtbDays>5</reproduceMtbDays>
			<wildClusterSizeRange>
				<min>8</min>
				<max>14</max>
			</wildClusterSizeRange>
			<wildClusterRadius>10</wildClusterRadius>
		</plant>
	</ThingDef>

