    <jobString>Making .338 Lapua Magnum (FMJ) cartridges.</jobString>
    <ingredients>
      <li>
        <filter>
          <categories>
			<li>SLDBar</li>
			<li>USLDBar</li>
          </categories>
        </filter>
        <count>14</count>
      </li>
      <li>
        <filter>
          <categories>
			<li>HeavyBar</li>
          </categories>
        </filter>
        <count>6</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Powder</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>SLDBar</li>
        <li>USLDBar</li>
		<li>HeavyBar</li>
      </categories>
      <thingDefs>
        <li>Powder</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Ammo_338Lapua_FMJ>80</Ammo_338Lapua_FMJ>
    </products>
		<skillRequirements>
			<Crafting>4</Crafting>
		</skillRequirements>
    <workSkill>Crafting</workSkill>
    <workSkillLearnFactor>0.8</workSkillLearnFactor>
  </RecipeDef>
  
